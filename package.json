{
  "name": "epotion",
  "version": "0.0.13",
  "description": "Detects face expressions and gathers the data for later analysis",
  "source": "src/epotion.js",
  "main": "dist/epotion.js",
  "unpkg": "dist/epotion.umd.js",
  "umd:main": "dist/epotion.umd.js",
  "module": "dist/epotion.m.js",
  "esmodule": "dist/epotion.modern.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "start": "microbundle watch",
    "build": "microbundle",
    "lint": "eslint './src/**/*.js'",
    "lint:fix": "eslint --fix './src/**/*.js'",
    "test": "jest --passWithNoTests --forceExit -- -coverage",
    "patch": "npm --no-git-tag-version version patch"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcinxkaminski/epotion.git"
  },
  "keywords": [
    "epotion",
    "emotion events"
  ],
  "author": "Marcin Kaminski <ma.kaminski+epotion@icloud.com> (https://github.com/marcinxkaminski)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcinxkaminski/epotion/issues"
  },
  "homepage": "https://github.com/marcinxkaminski/epotion#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-markdown": "^2.0.0-rc.0",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-plugin-xss": "^0.1.10",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "microbundle": "^0.12.4",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "face-api.js": "^0.22.2",
    "mitt": "^2.1.0"
  }
