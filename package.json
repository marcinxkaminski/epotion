{
  "name": "epotion",
  "version": "0.0.5",
  "description": "Detects face expressions and gathers the data for later analysis",
  "main": "src/epotion.js",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --progress --colors --mode=development --config webpack.config.cjs",
    "build:prod": "webpack --mode=production --config webpack.config.cjs",
    "build:dev": "webpack --mode=development --config webpack.config.cjs",
    "lint": "eslint './src/**/*.js'",
    "lint:fix": "eslint --fix './src/**/*.js'",
    "test": "jest --passWithNoTests --forceExit -- -coverage",
    "prepublishOnly": "npm run patch && npm run build:prod",
    "patch": "npm --no-git-tag-version version patch"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcinxkaminski/epotion.git"
  },
  "keywords": [
    "epotion",
    "emotion events"
  ],
  "author": "Marcin Kaminski <ma.kaminski+epotion@icloud.com> (https://github.com/marcinxkaminski)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcinxkaminski/epotion/issues"
  },
  "homepage": "https://github.com/marcinxkaminski/epotion#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@testing-library/jest-dom": "^5.11.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jest-dom": "^3.2.4",
    "eslint-plugin-markdown": "^2.0.0-rc.0",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-plugin-xss": "^0.1.10",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^5.0.0-alpha.6",
    "jest": "^26.6.2",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "face-api.js": "^0.22.2",
    "mitt": "^2.1.0"
  }
}
